{
  "name": "@kubekavach/ai",
  "version": "0.1.0",
  "description": "AI provider integration for KubeKavach",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist .turbo",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "dependencies": {
    "@kubekavach/core": "workspace:*",
    "openai": "^4.24.1",
    "@anthropic-ai/sdk": "^0.54.0",
    "@google/generative-ai": "^0.2.1",
    "axios": "^1.6.5",
    "ollama": "^0.5.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  }
}
